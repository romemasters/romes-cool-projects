  <script>
    // Cloudflare-style loading screen logic
    (() => {
      const loader = document.getElementById("cf-loader");
      let minTime = 1000, maxTime = 2000;
      let shownAt = Date.now();
      function hideLoader() {
        loader.classList.add('hide');
        setTimeout(() => loader.style.display = 'none', 600);
      }
      window.addEventListener('DOMContentLoaded', () => {
        const elapsed = Date.now() - shownAt;
        if (elapsed < minTime) {
          setTimeout(hideLoader, minTime - elapsed);
        } else {
          hideLoader();
        }
        setTimeout(hideLoader, maxTime); // in case page loads very fast
      });
    })();

    // Bookmarklet copy and save functions
    function copyBookmarklet(code, name) {
      try {
        const textarea = document.createElement('textarea');
        textarea.value = code;
        document.body.appendChild(textarea);
        textarea.select();
        document.execCommand('copy');
        document.body.removeChild(textarea);
        alert('Bookmarklet "' + name + '" copied to clipboard!');
      } catch (err) {
        alert('Failed to copy bookmarklet. Please copy manually from the code block.');
      }
    }

    function saveBookmarklet(url, name) {
      try {
        if (window.sidebar && window.sidebar.addPanel) { // Firefox
          window.sidebar.addPanel(name, url, '');
        } else if (window.external && window.external.AddFavorite) { // IE
          window.external.AddFavorite(url, name);
        } else { // Chrome, Safari, Edge, etc.
          alert('Please drag the "Save to Bookmarks" link to your bookmarks bar, or right-click and select "Add to Bookmarks".');
        }
      } catch (err) {
        alert('Automatic bookmarking not supported. Please drag the "Save to Bookmarks" link to your bookmarks bar.');
      }
    }
  </script>
